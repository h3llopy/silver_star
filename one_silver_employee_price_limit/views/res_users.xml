<odoo>
    <data>

        <record id="one_silver_employee_price_limit.res_user_form_view_inherit" model="ir.ui.view">
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group" position="after">
                    <group string="Employee Limit's">
                        <group>
                            <field name="user_limit_type"/>
                        </group>
                    </group>
                </xpath>
            </field>
        </record>
    </data>
</odoo>